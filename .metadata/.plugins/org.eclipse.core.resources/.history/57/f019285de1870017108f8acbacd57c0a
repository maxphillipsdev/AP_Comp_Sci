public class Exercise_2 {

	public static void main(String[] args) {
		System.out.format("%6s %6s","|","|\n");
		System.out.format("%6s %6s","|","|\n");
		System.out.format("%6s %6s","|","|\n");
		System.out.println("-----------------");
		System.out.format("%6s %6s","|","|\n");
		System.out.format("%6s %3s %6s","|","X","|\n");
		System.out.format("%6s %6s","|","|\n");
		

	}

}
