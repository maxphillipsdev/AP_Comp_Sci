/**
 * Exercise 1 Project
 * Prints a BingoCard to the console
 * @author 2020_maxwell_phillips
 * AP Computer Science
 * August 21 2017
 */
public class Exercise_1 {

	public static void main(String[] args) {
		System.out.format("%6s %6s %6s %6s %6s","B","I","N","G","O");
		System.out.format("%6s %6s %6s %6s %6s","2","20","42","60","64");
		System.out.format("%6s %6s %6s %6s %6s","14","25","32","55","70");
		System.out.format("%6s %6s %6s %6s %6s","","I","N","G","O");
		System.out.format("%6s %6s %6s %6s %6s","B","I","N","G","O");
		System.out.format("%6s %6s %6s %6s %6s","B","I","N","G","O");

	}

}
